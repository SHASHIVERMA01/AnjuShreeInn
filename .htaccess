RewriteEngine on
RewriteCond %{HTTP_HOST} ^pragnyaec.in [NC]
RewriteRule ^(.*)$ https://www.pragnyaec.in/$1 [L,R=301,NC]
Redirect 301 /index.php https://www.pragnyaec.in

RewriteCond %{REQUEST_URI} !^public
RewriteRule ^(.*)$ public/$1 [L]


